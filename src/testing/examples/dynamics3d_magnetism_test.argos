<?xml version="1.0" ?>
<argos-configuration>

  <!-- ************************* -->
  <!-- * General configuration * -->
  <!-- ************************* -->
  <framework>
    <experiment length="0" ticks_per_second="5" random_seed="1"/>
  </framework>

  <!-- *************** -->
  <!-- * Controllers * -->
  <!-- *************** -->
  <controllers />

  <!-- *********************** -->
  <!-- * Arena configuration * -->
  <!-- *********************** -->
  <arena size="0.25, 0.25, 0.125" center="0, 0, 0.0625">
    <distribute>
      <position method="grid" center="0,0,0" distances="0.025,0.05,0"
                layout="5,5,1" />
      <orientation method="grid" center="0,0,0" distances="0,0,0"
                   layout="5,5,1" />
      <entity quantity="25" max_trials="25">
        <prototype id="magnet" movable="true">
          <links ref="base">
            <link id="base" geometry="box" size=".015,.015,.015" mass="0.05"
                  position="0,0,0" orientation="0,0,0" />
          </links>
          <devices>
            <magnets>
              <magnet id="magnet" passive_field="-125,0,0" anchor="base" 
                      offset="0,0,.0075"/>
            </magnets>
          </devices>
        </prototype>
      </entity>
    </distribute>
  </arena>

  <!-- ******************* -->
  <!-- * Physics engines * -->
  <!-- ******************* -->
  <physics_engines>
    <dynamics3d id="dyn3d" iterations="20">
      <gravity g="9.8" />
      <floor height="0.01"/>
      <magnetism max_distance="0.075"/>
    </dynamics3d>
  </physics_engines>

  <!-- ********* -->
  <!-- * Media * -->
  <!-- ********* -->
  <media/>

  <!-- ****************** -->
  <!-- * Visualization * -->
  <!-- ****************** -->
  <visualization>
    <qt-opengl lua_editor="false">
      <camera>
        <placement idx="0" position=" 0,  -0.5,0.2" look_at="0,0,0.05" lens_focal_length="50" />
        <placement idx="1" position=" 0.5,-0.5,0.2" look_at="0,0,0.05" lens_focal_length="65" />
        <placement idx="2" position=" 0.5, 0,  0.2" look_at="0,0,0.05" lens_focal_length="50" />
        <placement idx="3" position=" 0.5, 0.5,0.2" look_at="0,0,0.05" lens_focal_length="65" />
        <placement idx="4" position=" 0,   0.5,0.2" look_at="0,0,0.05" lens_focal_length="50" />
        <placement idx="5" position="-0.5, 0.5,0.2" look_at="0,0,0.05" lens_focal_length="65" />
        <placement idx="6" position="-0.5, 0,  0.2" look_at="0,0,0.05" lens_focal_length="50" />
        <placement idx="7" position="-0.5,-0.5,0.2" look_at="0,0,0.05" lens_focal_length="65" />
        <placement idx="8" position="0,0,0.5" look_at="0.0,0.0,0.125" up="1,0,0" lens_focal_length="40" />
      </camera>
    </qt-opengl>
  </visualization>

</argos-configuration>
